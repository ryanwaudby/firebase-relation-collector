{
  "name": "firebase-relation-collector",
  "version": "1.0.2",
  "description": "collects all your relationship items in firebase",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanwaudby/firebase-relation-fetcher"
  },
  "license": "MIT",
  "options": {
    "mocha": "--require scripts/mocha_runner test/**/*.js"
  },
  "scripts": {
    "prepublish": ". ./scripts/prepublish.sh",
    "lint": "eslint ./src ./test",
    "lintfix": "eslint ./src --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-eslint": "4.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "chai": "3.x.x",
    "eslint": "1.7.x",
    "eslint-plugin-babel": "2.x.x",
    "firebase-server": "^0.5.4",
    "mocha": "2.x.x",
    "nodemon": "1.7.x"
  },
  "dependencies": {
    "babel-runtime": "6.x.x",
    "firebase": "^2.4.2",
    "ramda": "^0.20.1"
  }
}
